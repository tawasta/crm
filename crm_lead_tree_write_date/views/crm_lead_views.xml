<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="crm_case_tree_view_leads_write_date" model="ir.ui.view">
        <field name="name">crm.lead.tree.lead.write.date</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_case_tree_view_leads" />
        <field name="arch" type="xml">
            <field name="create_date" position="after">
                <field name="write_date" optional="show" />
            </field>
        </field>
    </record>
    <record id="crm_case_tree_view_oppor_write_date" model="ir.ui.view">
        <field name="name">crm.lead.tree.opportunity.write.date</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_case_tree_view_oppor" />
        <field name="arch" type="xml">
            <field name="create_date" position="after">
                <field name="write_date" optional="show" />
            </field>
        </field>
    </record>
</odoo>
